entrypoint = "main.py"
modules = ["python-3.11", "docker", "python-with-prybar-3.10", "nodejs-with-prybar-18", "web", "zig-0.11", "pyright", "vue-node-18", "typescript-language-server", "swift-5.6", "ruby-3.1", "rust-stable", "rust-nightly", "svelte-kit-node-20", "dotnet-10.0", "dotnet-8.0", "dotnet-9.0", "angular-node-20", "bash", "bun-1.2", "bun-1.1", "cpp-clang14", "clojure-1.11", "dart-3.8", "deno-2", "elixir-1_18", "dart-2.18", "clojure-1.12", "go-1.21", "go-1.20", "go-1.24", "haskell-ghc9.2", "haskell-ghc9.4", "r-4.2", "qbasic", "python-3.9", "python-3.8", "python-base-3.13", "python-base-3.12", "python-3.12", "python-base-3.11", "python-3.10", "r-4.5", "ruby-3.2", "postgresql-17", "php-8.1", "php-8.4", "nodejs-20", "nodejs-22", "nodejs-19", "nodejs-18", "nodejs-24", "nix", "lua-5.2", "hermit-0.38.2", "java-graalvm22.3", "nodejs-23", "haskell-ghc9.0", "haskell-ghc9.8", "deno-1", "elixir-1_15", "c-clang14", "bun-1.0"]

[nix]
channel = "stable-25_05"
packages = ["cargo", "libiconv", "libxcrypt", "openssl", "pkg-config", "postgresql", "rustc"]

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "shadow_bot"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Telegram Bot"

[[workflows.workflow]]
name = "shadow_bot"
author = "agent"
mode = "parallel"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "shadow_bot"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Telegram Bot"
author = "agent"
mode = "parallel"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python bot.py"
